<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E1F0FF"
    tools:context=".Registration">

    <Button
        android:id="@+id/reg_b"
        android:layout_width="270dp"
        android:layout_height="85dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="680dp"
        android:layout_marginEnd="68dp"
        android:backgroundTint="#6E85FB"
        android:fontFamily="@font/cormorant_sc_light"
        android:text="Зареєструватись"
        android:textSize="28sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/cormorant_infant_light"
        android:text="ім'я:"
        android:textColor="#6E85FB"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/enter_name"
        style="@style/Widget.Material3.AutoCompleteTextView.FilledBox.Dense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/cormorant_infant_light"
        android:hint="Введіть ім'я"
        android:inputType="text|textPersonName"
        android:lineSpacingExtra="8sp"
        android:padding="10dp"
        android:textAlignment="gravity"
        android:textColorHint="#AFCEE9"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name_l" />

    <TextView
        android:id="@+id/login_l2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/cormorant_infant_light"
        android:text="пошта:"
        android:textColor="#6E85FB"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enter_name" />

    <EditText
        android:id="@+id/enter_login"
        style="@style/Widget.Material3.AutoCompleteTextView.FilledBox.Dense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/cormorant_infant_light"
        android:hint="Введіть пошту"
        android:inputType="text|textEmailAddress"
        android:lineSpacingExtra="8sp"
        android:padding="10dp"
        android:textAlignment="gravity"
        android:textColorHint="#AFCEE9"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/login_l2" />

    <TextView
        android:id="@+id/passwd_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/cormorant_infant_light"
        android:text="пароль:"
        android:textColor="#6E85FB"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enter_login" />

    <EditText
        android:id="@+id/enter_passwd"
        style="@style/Widget.Material3.AutoCompleteTextView.FilledBox.Dense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/cormorant_infant_light"
        android:hint="Введіть пароль"
        android:inputType="text|textPassword"
        android:lineSpacingExtra="8sp"
        android:padding="10dp"
        android:textAlignment="gravity"
        android:textColorHint="#AFCEE9"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwd_l" />

    <TextView
        android:id="@+id/phone_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/cormorant_infant_light"
        android:text="телефон:"
        android:textColor="#6E85FB"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enter_passwd" />

    <EditText
        android:id="@+id/enter_phone"
        style="@style/Widget.Material3.AutoCompleteTextView.FilledBox.Dense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/cormorant_infant_light"
        android:hint="Введіть телефон"
        android:inputType="number"
        android:lineSpacingExtra="8sp"
        android:padding="10dp"
        android:textAlignment="gravity"
        android:textColorHint="#AFCEE9"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.533"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_l" />

    <TextView
        android:id="@+id/adress_l"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:fontFamily="@font/cormorant_infant_light"
        android:text="адрес:"
        android:textColor="#6E85FB"
        android:textSize="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enter_phone" />

    <EditText
        android:id="@+id/enter_adress"
        style="@style/Widget.Material3.AutoCompleteTextView.FilledBox.Dense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="68dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:fontFamily="@font/cormorant_infant_light"
        android:hint="Введіть адресу.."
        android:inputType="text"
        android:lineSpacingExtra="8sp"
        android:padding="10dp"
        android:textAlignment="gravity"
        android:textColorHint="#AFCEE9"
        android:textSize="18sp"
        android:typeface="normal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.466"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adress_l" />

</androidx.constraintlayout.widget.ConstraintLayout>